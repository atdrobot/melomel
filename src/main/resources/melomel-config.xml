<flex-config>
	<output>../../../target/melomel-0.4.2.swc</output>
	<target-player>10.0.0</target-player>
	<benchmark>false</benchmark>
	<warnings>true</warnings>

	<compiler>
		<debug>true</debug>

		<keep-generated-actionscript>false</keep-generated-actionscript>

		<source-path>
			<path-element>../flex</path-element>
		</source-path>

		<namespaces>
			<namespace>
				<uri>library://melomel/2010</uri>
				<manifest>manifest.xml</manifest>
			</namespace>
		</namespaces>
	</compiler>

	<include-classes>
		<class>MelomelClasses</class>
	</include-classes>

	<metadata>
		<creator>Ben Johnson</creator>
		<contributor>Ben Johnson</contributor>
		<description>A library for controlling Flash objects through an external interface.</description>
		<language>EN</language>
		<title>melomel</title>
	</metadata>
</flex-config>
